{
	"id": "atrust",
	"name": "A-Trust Mobile phone signature",
	"tags": ["connector"],
	"mavenArtifacts": [
		{
			"name": "A-Trust Connector",
			"groupId": "com.axonivy.connector.atrust",
			"artifactId": "a-trust-connector",
			"makesSenseAsMavenDependency": true
		},
		{
			"name": "A-Trust Connector Demo",
			"groupId": "com.axonivy.connector.atrust",
			"artifactId": "a-trust-connector-demo"
		}
	],
	"installers": [
		{
			"id": "maven-dependency",
			"data": {
				"dependencies": [
					{
						"groupId": "com.axonivy.connector.atrust",
						"artifactId": "a-trust-connector",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories":[
					{
						"id":"axonivy.public",
						"url":"http://repo.axonivy.rocks",
						"snapshots":{"enabled":"true"}
					}
				]
			}
		},
		{
			"id": "maven-import",
			"data": {
				"projects":[
					{
						"groupId": "com.axonivy.connector.atrust",
						"artifactId": "a-trust-connector-demo",
						"version": "${version}",
						"type": "iar"
					}
				],
				"repositories":[
					{
						"id":"axonivy.public",
						"url":"http://repo.axonivy.rocks"
					}
				]
			}
		},
		{
			"id": "open-resource",
			"data": {
				"project": "atrust-connector-demo",
				"resources": [
					"processes/ATrustIntegration/DemoStart.mod"
				]
			}
		}
	]
}
